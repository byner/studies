def table6():
    list=[]
    index=1
    while index<=10:
        list.append(6*index)
        index=index+1
    return list
print(table6())

def table6Quadruple():
    print(table6())
    print(table6())
    print(table6())
    print(table6())
table6Quadruple()

def tableN(arg):
    list=[]
    index=1
    while index<=10:
        list.append(arg*index)
        index=index+1
    return list
print(tableN(int(input("Quelle table de multiplication ? "))))

def tableNRepeated(arg1,arg2):
    index=1
    while index<=arg2:
        print(tableN(arg1))
        index=index+1
tableNRepeated(int(input("Quelle table de multiplication ? ")),int(input("Sur combien de lignes ? ")))    
